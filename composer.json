{
    "name": "poniverse/pony.fm",
    "description": "A community for pony fan music.",
    "keywords": [
        "music",
        "pony",
        "php",
        "laravel"
    ],
    "license": "AGPL",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.*",
        "codescale/ffmpeg-php": "2.7.0",
        "barryvdh/laravel-ide-helper": "v2.8.0",
        "guzzlehttp/guzzle": "~6.0",
        "doctrine/dbal": "2.5.*",
        "venturecraft/revisionable": "^1.36",
        "pda/pheanstalk": "~3.0",
        "cviebrock/laravel-elasticsearch": "4.0.0",
        "barryvdh/laravel-debugbar": "~3.4",
        "predis/predis": "^1.0",
        "ksubileau/color-thief-php": "^1.3",
        "graham-campbell/exceptions": "^12.0",
        "minishlink/web-push": "^1.0",
        "alsofronie/eloquent-uuid": "^1.0",
        "poniverse/api": "dev-rewrite",
        "fruitcake/laravel-cors": "2.0.1",
        "laravel/tinker": "^1.0",
        "doctrine/collections": "v1.4.*",
        "doctrine/annotations": "v1.4.*",
        "doctrine/cache": "v1.6.*",
        "doctrine/instantiator": "1.0.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "filp/whoops": "^2.0",
        "symfony/dom-crawler": "~3.1",
        "symfony/css-selector": "~3.1",
        "laravel/browser-kit-testing": "2.*",
        "nategood/httpful": "^0.2.20",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/factories",
            "database/migrations",
            "database/seeds",
            "app/Library"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
